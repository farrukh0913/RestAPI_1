{
    "openapi": "3.0.0",
    "info": {
        "title": "APIs for Demo App",
        "version": "1.0.0"
    },
    "paths": {
        "/createStudent": {
            "post": {
                "summary": "Create a new student",
                "operationId": "2e4b91a63ffee99573de7a9ead68de94",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "first_name": {
                                        "description": "Student's name",
                                        "type": "string",
                                        "example": "John Doe"
                                    },
                                    "last_name": {
                                        "description": "Student's name",
                                        "type": "string",
                                        "example": "John Doe"
                                    },
                                    "email": {
                                        "description": "Student's grade",
                                        "type": "string",
                                        "example": "A"
                                    },
                                    "birthdate": {
                                        "description": "Student's address",
                                        "type": "string",
                                        "example": "123 Main St"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    }
                }
            }
        },
        "/getAllStudents": {
            "get": {
                "summary": "Get example data",
                "operationId": "383a2f1941f5e35e92bca99bb13b1f8c",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    }
                }
            }
        },
        "/deleteStudent/{id}": {
            "delete": {
                "summary": "Delete a student by ID",
                "operationId": "4b02e3a6417ddaa692fdab0de5bbf717",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the student to be deleted",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "404": {
                        "description": "Student not found"
                    }
                }
            }
        },
        "/users": {
            "get": {
                "summary": "Get example data",
                "operationId": "ccf4de46931338f0a533c3710b21d59c",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "name": "bearerAuth",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    }
}